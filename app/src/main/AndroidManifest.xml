<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.khoirullatif.footballstandings">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FootballStudents">
        <activity android:name="com.khoirullatif.footballstandings.ui.standing.StandingActivity"></activity>
        <activity android:name="com.khoirullatif.footballstandings.ui.season.SeasonActivity" />
        <activity android:name="com.khoirullatif.footballstandings.ui.home.HomeActivity" />
        <activity android:name="com.khoirullatif.footballstandings.ui.login.LoginActivity" />
        <activity android:name="com.khoirullatif.footballstandings.ui.onboarding.OnBoardingActivity" />
        <activity android:name="com.khoirullatif.footballstandings.ui.splashscreen.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.khoirullatif.footballstandings.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>